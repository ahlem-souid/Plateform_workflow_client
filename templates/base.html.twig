<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets %}
        <link href="{{ asset('trust/img/favicon.png') }}" rel="icon">
        <link href="{{ asset('trust/img/apple-touch-icon.png') }}" rel="apple-touch-icon">

        <!-- Bootstrap core CSS -->
        <!--external css-->
        <link href="{{ asset('trust/lib/font-awesome/css/font-awesome.css') }}" rel="stylesheet"/>

        <link rel="stylesheet" type="text/css" href="{{ asset('trust/lib/gritter/css/jquery.gritter.css') }}"/>
        <link href="{{ asset('trust/css/style.css') }}" rel="stylesheet">
         <link href="{{ asset('trust/css/login.css') }}" rel="stylesheet">
        <link href="{{ asset('trust/css/style-responsive.css') }}" rel="stylesheet">
        <script src="{{ asset('trust/lib/chart-master/Chart.js') }}"></script>
        
    {% endblock %}
<style type="text/css">
    .non-vue{
        background-color: #3978a2;
    color: white;
    }
</style>

</head>

<body>
{% block body %}
    {% if not is_granted('IS_AUTHENTICATED_REMEMBERED') %}
        <div>
            <a href="{{ path('app_register') }}">Register</a>
        </div>
    {% endif %}

{% endblock %}
{% block javascripts %}

<script src="{{ asset('trust/lib/jquery/jquery.min.js') }}"></script>

<script src="{{ asset('trust/lib/bootstrap/js/bootstrap.min.js') }}"></script>
<script class="include" type="text/javascript" src="{{ asset('trust/lib/jquery.dcjqaccordion.2.7.js') }}"></script>
<script src="{{ asset('trust/lib/jquery.scrollTo.min.js') }}"></script>
<script src="{{ asset('trust/lib/jquery.nicescroll.js') }}" type="text/javascript"></script>
<script src="{{ asset('trust/lib/jquery.sparkline.js') }}"></script>
<!--common script for all pages-->
<script src="{{ asset('trust/lib/common-scripts.js') }}"></script>
<script type="text/javascript" src="{{ asset('trust/lib/gritter/js/jquery.gritter.js') }}"></script>
<script type="text/javascript" src="{{ asset('trust/lib/gritter-conf.js') }}"></script>
<!--script for this page-->


{% endblock %}
</body>
</html>